{
   "name": "vscode-numworks",
   "displayName": "VSCode Numworks",
   "description": "",
   "publisher": "k-kuroguro",
   "version": "0.0.1",
   "icon": "resources/icon.png",
   "engines": {
      "vscode": "^1.58.0"
   },
   "categories": [
      "Other"
   ],
   "license": "MIT",
   "homepage": "https://github.com/k-kuroguro/vscode-numworks/blob/master/README.md",
   "repository": {
      "type": "git",
      "url": "https://github.com/k-kuroguro/vscode-numworks.git"
   },
   "bugs": {
      "url": "https://github.com/k-kuroguro/vscode-numworks/issues"
   },
   "activationEvents": [
      "*"
   ],
   "main": "./dist/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "vscode-numworks.openSimulator",
            "title": "Open Simulator",
            "category": "VSCode Numworks"
         },
         {
            "command": "vscode-numworks.openPythonSimulator",
            "title": "Open Python Simulator",
            "category": "VSCode Numworks"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "npm run webpack:prod",
      "webpack:dev": "webpack --mode development",
      "webpack:prod": "webpack --mode production --devtool hidden-source-map",
      "webpack:watch": "webpack --watch",
      "watch": "npm run webpack:watch",
      "tsc": "tsc -p ./",
      "tsc:watch": "tsc -watch -p ./",
      "test-compile": "run-p webpack:dev tsc",
      "test-watch": "run-p webpack:watch tsc:watch",
      "pretest": "npm run test-compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
   },
   "devDependencies": {
      "@types/copy-webpack-plugin": "^8.0.1",
      "@types/express": "^4.17.13",
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.4",
      "@types/node": "^12.11.7",
      "@types/vscode": "^1.57.0",
      "@typescript-eslint/eslint-plugin": "^4.14.1",
      "@typescript-eslint/parser": "^4.14.1",
      "copy-webpack-plugin": "^9.0.1",
      "eslint": "^7.19.0",
      "mocha": "^8.2.1",
      "npm-run-all": "^4.1.5",
      "ts-loader": "^8.0.14",
      "typescript": "^4.1.3",
      "vscode-test": "^1.5.0",
      "webpack": "^5.19.0",
      "webpack-cli": "^4.4.0"
   },
   "dependencies": {
      "ejs": "^3.1.6",
      "express": "^4.17.1",
      "node": "^16.4.0"
   }
}
